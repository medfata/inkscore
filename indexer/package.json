{
  "name": "ink-indexer",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/hybrid-indexer.js",
    "start:backfill": "node dist/hybrid-indexer.js",
    "start:realtime": "node dist/realtime-service.js",
    "start:enrichment": "node dist/enrichment-service.js",
    "start:event-enrichment": "node dist/event-driven-enrichment-service.js",
    "start:multi-enrichment": "node dist/multi-node-enrichment-service.js",
    "start:hybrid": "node dist/hybrid-indexer.js",
    "start:legacy": "node dist/index.js",
    "start:v2": "node dist/index-v2.js",
    "dev": "tsx watch src/hybrid-indexer.ts",
    "dev:hybrid": "tsx watch src/hybrid-indexer.ts",
    "dev:realtime": "tsx watch src/realtime-service.ts",
    "dev:enrichment": "tsx watch src/enrichment-service.ts",
    "dev:event-enrichment": "tsx watch src/event-driven-enrichment-service.ts",
    "dev:multi-enrichment": "tsx watch src/multi-node-enrichment-service.ts",
    "dev:legacy": "tsx watch src/index.ts",
    "dev:v2": "tsx watch src/index-v2.ts",
    "manual": "tsx src/manual-index.ts",
    "status": "tsx src/scripts/check-backfill-status.ts",
    "fix:function-names": "tsx src/fix-empty-function-names.ts",
    "db:migrate": "node dist/db/migrate.js",
    "db:migrate:v2": "node dist/db/migrate-v2.js",
    "db:migrate:hybrid": "psql $DATABASE_URL -f src/db/migrate-hybrid.sql",
    "db:migrate:enrichment": "psql $DATABASE_URL -f src/db/migrate-enrichment.sql",
    "enrichment": "tsx src/enrichment-service.ts",
    "gap-enrich": "tsx src/scripts/enrich-missing-transactions.ts",
    "concurrent-enrich": "tsx src/scripts/concurrent-gap-enrichment.ts",
    "verify-gaps": "tsx src/scripts/verify-enrichment-gaps.ts",
    "test": "node --import tsx --test src/services/__tests__/*.test.ts",
    "test:backfill": "node --import tsx --test src/services/__tests__/BackfillService.test.ts",
    "test:realtime": "node --import tsx --test src/services/__tests__/RealtimeService.test.ts"
  },
  "dependencies": {
    "adm-zip": "^0.5.16",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "pg": "^8.13.0",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/csv-parse": "^1.1.12",
    "@types/node": "^22.0.0",
    "@types/pg": "^8.11.10",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}